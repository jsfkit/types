{
  "name": "@jsfkit/types",
  "version": "0.1.0",
  "description": "A JSON representation for spreadsheets",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "check": "npm run format:check & npm run lint & npm run typecheck",
